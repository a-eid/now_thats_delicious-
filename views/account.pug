extends layout 

block content 
  .inner 
    h2= title 
    form.form(action="/account" method="POST")
      label(for="name") Name 
      input#name(name="name" value=user.name)
      label(for="email") Email
      input#email(name="email" type="email" value=user.email)
      input.button(type="submit" value="Update MY Account")
